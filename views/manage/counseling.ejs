                        <table class="counsel_history_table table table-striped" style="text-align: center; margin-top:1%;" id="counsel_history_table">
                            <thead>
                                <tr>
                                    <th scope="col" style="width: 5%;">No</th>
                                    <th scope="col" style="width: 5%;">ID</th>
                                    <th scope="col" style="width: 10%;">상담사 명</th>
                                    <th scope="col" style="width: 10%;">코칭 유형</th>
                                    <th scope="col" style="width: 10%;">기준 시간</th>
                                    <th scope="col" style="width: 30%;">코칭 내역</th>
                                    <th scope="col" style="width: 30%;">특이사항</th>
                                </tr>
                            </thead>

                            <tbody>
                                <% if(counsel_history.length == 0) { %> 
                                    <tr id="hide"><td colspan="7">표시할 데이터가 없습니다</td></tr>
                                <% } else { %>
                                    <!-- 조회시 ajax를 통해 테이블 출력 -->
                                    <% for(var i=0; i< counsel_history.length; i++) {
                                        let data = counsel_history[i]; %>

                                        <tr id="hide">
                                            <!-- 시퀀스 -->
                                            <td scope="row"><%= i+1 %></td>

                                            <!-- 상담사 ID -->
                                            <% let data_agentId = data.agent_id; %>
                                            <td scope="row"><%= data_agentId %></td>

                                            <!-- 상담사 이름 -->
                                            <% let data_agentName = data.agent_name; %>
                                            <td scope="row"><%= data_agentName %></td>

                                            <!-- 코칭 유형(자/수동) -->
                                            <% let data_autoCoach = data.auto_coach; %>
                                            <td scope="row"><%= data_autoCoach %></td>

                                            <!-- 조건 설정 시간 -->
                                            <% let data_autoStandard = data.auto_standard; %>
                                            <td scope="row"><%= data_autoStandard %>초</td>

                                            <!-- 코칭 내역 -->
                                            <% let data_autoDetail = data.auto_detail; %>
                                            <td scope="row"><%= data_autoDetail %></td>

                                            <!-- 특이사항 -->
                                            <% 
                                                let data_passEtc = data.pass_etc;

                                                if(data.pass_etc == null | data.pass_etc == "") {
                                                    data_passEtc = '시스템 자동 발신';
                                                }
                                            %>
                                            <td scope="row"><%= data_passEtc %></td>
                                        </tr>
                                    <% } %>
                                <% } %>
                            </tbody>
                        </table>

                        <!-- 페이징 처리 -->
                        <ul class="pagination3"></ul>
                    </div>
                </div>
