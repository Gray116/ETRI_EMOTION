                                        <thead>
                                            <tr>
                                                <th scope="col" style="width: 5%;">번호</th>
                                                <th scope="col" style="width: 15%;">날짜</th>
                                                <th scope="col" style="width: 15%;">기준 시간(초)</th>
                                                <th scope="col" style="width: 15%;">이름</th>
                                                <th scope="col" style="width: 15%;">화남(초과)</th>
                                                <th scope="col" style="width: 15%;">슬픔(초과)</th>
                                                <th scope="col" style="width: 10%;">통화 건수</th>
                                                <th scope="col" style="width: 10%;"></th>
                                            </tr>
                                        </thead>
            
                                        <tbody>
                                            <% if(condition_passive.length == 0) { %> 
                                                <tr><td colspan="8">표시할 데이터가 없습니다</td></tr>
                                            <% } else { %>
                                                <% for(var i=0; i< condition_passive.length; i++) {
                                                    let data = condition_passive[i]; %>
            
                                                    <tr>
                                                        <!-- 시퀀스 -->
                                                        <td scope="row"><%= i+1 %></td>
            
                                                        <!-- 날짜 -->
                                                        <% 
                                                            let data_callDate = data.call_date;
                                                            
                                                            let y = data_callDate.substr(0, 4);
                                                            let m = data_callDate.substr(4, 2);
                                                            let d = data_callDate.substr(6, 2);
            
                                                            let yMd = `${y}년 ${m}월 ${d}일`;    
                                                        %>
                                                        <td scope="row" id="getAgentDate"><%= yMd %></td>
            
                                                        <!-- 기준 시간(초)에 따라 시간 더해서 표출 -->
                                                        <%
                                                            let data_callTime = data.call_time;
                                                            let data_callTimeStan = data.call_timeStan.replace(/:/gi, "");
            
                                                            let h = data_callTime.substr(0, 2);
                                                            let min = data_callTime.substr(3, 2);
                                                            let s = data_callTime.substr(6, 2);
                                                                                                        
                                                            let yMd2 = `${h}:${min}:${s}`;
            
                                                            let h2 = data_callTimeStan.substr(0, 2);
                                                            let min2 = data_callTimeStan.substr(2, 2);
                                                            let s2 = data_callTimeStan.substr(4, 2);
            
                                                            let yMd3 = `${h2}:${min2}:${s2}`;
                                                        %>
                                                        <td scope="row" id="getAgentTime"><%= yMd2 + ' ~ ' + yMd3 %></td>
            
                                                        <!-- 이름 -->
                                                        <td scope="row" id="getAgentName"><%= data.agent_name %></td>
            
                                                        <!-- 화남 조건 -->
                                                        <td scope="row"><%= data.auto_over_anger%>회</td>
            
                                                        <!-- 슬픔 조건 -->
                                                        <td scope="row"><%= data.auto_over_sad%>회</td>
            
                                                        <!-- 통화 건수 -->
                                                        <td scope="row"><%= data.call_count%>건</td>
            
                                                        <!-- 코칭 메세지 팝업 창 호출 -->
                                                        <td scope="row"><button class="agent_coach_btn btn btn-sm btn-outline-warning" type="submit">코칭하기</button></td>
                                                    </tr>
                                                <% } %>
                                            <% } %>                                   
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </form>

                        <!-- 페이징 처리 -->
                        <ul class="pagination2"></ul>
                    </div>

                    <!-- 상담사 코칭 이력 -->
