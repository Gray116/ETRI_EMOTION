    <!-- 상담사 수동코칭 이름 값 넘겨서 테이블에 표시 -->
    <script src="/socket.io/socket.io.js"></script>
    <script type="text/javascript">
        $('input[name="dates"]').daterangepicker();

        // 상담관리자 웹소켓 연결
        const socket = io.connect('https://211.41.186.209:8888', { path: '/socket.io', transports: ['websocket'] });
        let agent_userName;

        socket.on('connect', () => { console.log('상담관리자 연결 성공'); });

        // 연결 해제(disconnect) 이벤트 처리
        socket.on('disconnect', (reason) => { console.log(reason); });

        // 에러 발생(error) 이벤트 처리
        socket.on('error', (error)=>{ console.log(`오류 발생: ${error}`); });

        // 웹 소켓 서버에게 상담사 이름 보내기 (상담관리자)
        $(".agent_coach_btn").click(function() {
            agent_userName = document.querySelector('#getAgentName').innerText;
            agent_callDate = document.querySelector('#getAgentDate').innerText;
            agent_callTime = document.querySelector('#getAgentTime').innerText;
            
            socket.emit('client_name', `${agent_callDate}`);
            socket.emit('client_name', `${agent_callTime}`);
            socket.emit('client_name', `${agent_userName}`);

            movePage2();
        })
    </script>

    <!-- 코칭 조건 설정 CRUD -->
    <script type="text/javascript">
        let tdArr = new Array();

        // 행 클릭
        $("#auto_counsel_table tbody tr").click(function() {
            if ($(this).find("input").prop("checked")) {
                $(this).find("input").prop("checked", false);
            } else {
                $(this).find("input").prop("checked", true);
            }
        });

        // 코칭 이력 검색
        $(".counsel_search_btn").on("click", function() {    
            let select_myStatus        = document.getElementById('counsel_history_select').value;
            let select_myTime          = document.getElementById('counsel_history_date').value;

            // ajax로 여기에 테이블 조건 데이터 입력해야함
            // 처음 페이지 렌더링 될 때는 오늘 날짜 기준으로 렌더링
            $.ajax({
                url: '/manage/counsel_history?stat_selected='+select_myStatus+'&dates='+select_myTime,
                type: 'GET',
                dataType: 'json',
                contentType: 'application/json',
                success: function(data) {
                    if(data.message === 'SUCCESS') {
                        if(data.counsel_history == "") {
                            $('#counsel_history_table tbody tr').empty();             

                            let row = `<tr id="hide"><td colspan="7">표시할 데이터가 없습니다</td></tr>`;
                            $('.counsel_history_table').append(row);
                        } else {
                            $('#counsel_history_table tbody tr').empty();

                            // 데이터 길이만큼 ROW 추가
                            for (let i=0; i<data.counsel_history.length; i++) {
                                // EJS로 설정된 첫 행 삭제
                                let hide_row = document.getElementById('hide');
