        // 선택된 행 삭제 및 전체 삭제
        $(".counsel_del_btn").click(function() {
            let checkbox    = $("input[name=row_CheckBox]:checked");

            if(checkbox.length == 0) {
                alert('선택된 조건이 없습니다.');

                return false;
            }

            checkbox.each(function(i) {
                // checkbox.parent()            : checkbox의 부모는 <td>
                // checkbox.parent().parent()   : <td>의 부모이므로 <tr>
                let tr = checkbox.parent().parent().eq(i);
                let td = tr.children();
                
                tdArr.push(td.eq(1).text());
            })

            $.ajax({
                url: '/manage/counseling_deleting',
                dataType: 'json',
                type: 'GET',
                data: {data:tdArr},
                success: function(result) {
                    if(result) {
                        logger.info(result);
                        alert('정상적으로 삭제되었습니다.');
                        
                        window.close();
                    }
                }
            });

            location.reload();
        });

